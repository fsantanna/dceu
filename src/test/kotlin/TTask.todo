    @Test
    fun ff_pool20_throw() {
        val out = ceu.all(
            """
            var ts
            set ts = tasks(2)
            var T
            set T = task (v) {
                defer {
                    println(v)
                }
                catch err==:ok {
                    spawn task () {
                        yield(nil)
                        if v == 1 {
                            throw(:ok)
                        } else {
                            nil
                        }
                        loop { yield(nil) }
                    } ()
                    loop { yield(nil) }
                }
                println(v)
            }
            spawn in ts, T(1)
            spawn in ts, T(2)
            broadcast nil
            broadcast nil
            println(999)
        """
        )
        assert(out == "1\n1\n999\n2\n") { out }
    }
